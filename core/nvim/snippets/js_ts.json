{
  "console.log": {
    "prefix": ["con", "cl"],
    "body": ["console.log($1);"],
    "description": "Log output to console"
  },
  "Export Default": {
    "prefix": ["export default"],
    "body": ["export default function ${TM_FILENAME_BASE}() {", "  $0", "}"],
    "description": "Creates a default export function with the function name set to the current file's base name"
  },
  "Use Effect": {
    "prefix": ["useEffect"],
    "body": ["useEffect(() => {", "  $1", "}, [$2]);"]
  }
}

general {
  colors = false
  interval = 5
}

order += "wireless _first_"
order += "battery all"
order += "tztime local"

wireless _first_ {
  format_up = "Wi-Fi: %ip"
  format_down = "Wi-Fi: down"
}

battery all {
  format = "Battery: %percentage %remaining"
}

tztime local {
  format = "%Y-%m-%d %H:%M:%S"
}


